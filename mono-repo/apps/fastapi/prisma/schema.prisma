generator client {
  provider = "prisma-client-py"
}

datasource db {
  provider = "postgresql"   // or "mysql" / "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  name      String?
  createdAt DateTime @default(now())
}
